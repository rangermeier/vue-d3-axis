{
  "name": "vue-d3-axis",
  "version": "0.1.0",
  "description": "Vue-based Axis component for D3",
  "repository": "https://github.com/rangermeier/vue-d3-axis.git",
  "author": "seevee",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-d3-axis.umd.cjs",
  "module": "./dist/vue-d3-axis.js",
  "exports": {
    ".": {
      "import": "./dist/vue-d3-axis.js",
      "require": "./dist/vue-d3-axis.umd.cjs"
    }
  },
  "types": "src/types.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "npm run build",
    "setup": "npm install"
  },
  "peerDependencies": {
    "d3-scale": "^4.0.2",
    "vue": "^3.4.35"
  },
  "devDependencies": {
    "@types/node": "^22.2.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/test-utils": "^2.4.6",
    "d3-scale": "^4.0.2",
    "jsdom": "^24.1.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.0",
    "vitest": "^2.0.5",
    "vue": "^3.4.35",
    "vue-tsc": "^2.0.29"
  },
  "keywords": [
    "vue",
    "component",
    "vue-component",
    "ui",
    "d3",
    "d3-axis",
    "d3-scale"
  ]
}
